! ===================
!       urxvt
! ===================

URxvt*scrollBar:	    false
URxvt*letterSpace:  	-1
URxvt.internalBorder: 10
URxvt*font: 		      xft:Fira Code:size=12
URxvt*boldFont:		    xft:Fira Code:size=12
URxvt*cursorBlink:    true
URxvt.iso14755:		    false
URxvt*utf8:			      true

